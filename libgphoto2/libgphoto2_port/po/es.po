msgid "low"
msgstr "bajo"

#: libgphoto2_port/gphoto2-port.c:687
msgid "high"
msgstr "alto"

#: libgphoto2_port/gphoto2-port.c:1215
msgid "No error description available"
msgstr "No está disponible la descripción del error"

