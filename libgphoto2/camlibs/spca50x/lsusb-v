#!/usr/bin/perl -w
# lsusb-v
#####################################################################
#
# This file contains the output of lsusb -v for the cameras 
# supported by this driver
#
# The purpose is to have a comprehensive database in which to find 
# distinguishing properties for cameras with the same ProductID but 
# different features
#
# Add data after the __DATA__ line. There is a template you can copy.
#
# Run this file with perl to create the lsusb output in extra files.
# 
######################################################################

use strict;
my $dir = "lsusb-v-comparison";

mkdir($dir) || die "no mkdir";

my $print = 0;
my $fname = 0;
while (<DATA>) {
	my $line = $_;
	if ($line =~ /^##BEGIN lsusb-v/) {
		$print = 1;
		open(OUT, ">${dir}/${fname}");
		$fname++;
	} elsif ($line =~ /^##END lsusb-v/) {
		$print = 0;
		close(OUT);
	}
	if ($print) {
		print OUT $line;
	}
}
print "Have a look at ${dir}/* now :-)\n";

__DATA__


##BEGIN CAMERA
#Camera name: <whatever your camera is called
#Media: <sdram, flash, storage card, etc.>
#Contact person: <your name and possibly
##BEGIN Notes
# <insert your remarks about the camera here>
##END Notes
##BEGIN lsusb-v
<output of "lsusb -v">
##END lsusb-v
##END CAMERA



